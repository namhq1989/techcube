{
  "name": "techcube",
  "version": "1.0.0",
  "description": "Techcube outsource",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"NODE_ENV=development DEBUG=App,$DEBUG babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src",
    "lint": "eslint --ext .js src",
    "precommit": "npm run lint",
    "precommit-msg": "echo 'Pre-commit checks ...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "precommit"
  ],
  "author": "Nam Huynh <namhq.1989@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.13.3",
    "cli-table": "^0.3.1",
    "compress": "^0.99.0",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "diacritics": "^1.3.0",
    "express": "^4.13.3",
    "express-xml-bodyparser": "^0.3.0",
    "helmet": "^3.9.0",
    "ip": "^1.1.5",
    "method-override": "^2.3.10",
    "moment": "^2.19.2",
    "mongoose": "^4.13.5",
    "morgan": "^1.8.0",
    "public-ip": "^2.3.5",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "nodemon": "^1.9.2",
    "pre-commit": "^1.2.2"
  }
}
